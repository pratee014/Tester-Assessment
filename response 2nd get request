    if(glid) pers_widget(glid);
});

function pers_widget(glid)
{
    if(typeof(perWidget)!="undefined")
    {
        PersonalizedWidget({widgetID:'W3_INS',modID:'DIR_NotFound',glusrID:glid});
        PersonalizedWidget({widgetID:'W4_INS',modID:'DIR_NotFound',glusrID:glid});
        PersonalizedWidget({widgetID:'W2_INS',modID:'DIR_NotFound',glusrID:glid});
        if(typeof(glid)!="undefined" && glid != '' )
        {
            PersonalizedWidget({widgetID:'W5',modID:'DIR_NotFound',glusrID:glid});
        }
    }
    else
    {
        setTimeout(function(){pers_widget(glid)},50);
    }
}
 var cnt_tim = setInterval(function() {
        bljsloadcheck()
    }, 1000);
    setTimeout(function() {
        clearInterval(cnt_tim)
    }, 30000);
function bljsloadcheck()
{
if(typeof isBLFormGlobalJSLoaded != 'undefined' && isBLFormGlobalJSLoaded == 1)
{
clearInterval(cnt_tim);
notfoundtrack();
}
}
</script>
<script type="text/javascript" language="javascript">
var styletimer = setInterval(function() {
        loadstyle()
    }, 500);

function loadstyle()
{
var i='',tagval='';
var blcss = new Array();
var tag = document.getElementsByTagName('link');
for(i=0;i<tag.length;i++)
{
tagval=tag[i];
var blcss = tagval.getAttribute('href');
if(blcss != 'undefined' && blcss != null && blcss != '' && blcss.indexOf('apps.imimg.com/css/blEnqForm2.css')>0)
{
 $('.stky').slideDown().delay(1000);
 clearInterval(styletimer);
}
}
}

</script>

