    function replaceNullWithEmptyStr(obj) {
        if (obj && typeof obj === 'object') {
            // Iterate over key-value pairs and replace null values with empty strings
            Object.entries(obj).forEach(([key, value]) => {
                obj[key] = value == null ? '' : value;
            });
        }
        return obj;
    }

    function getObject(row, relProdType) {
        const dd = new Date();
        const cr_dt = `${dd.getFullYear()}${(dd.getMonth() + 1).toString().padStart(2, '0')}${dd.getDate().toString().padStart(2, '0')}${dd.getHours().toString().padStart(2, '0')}${dd.getMinutes().toString().padStart(2, '0')}${dd.getSeconds().toString().padStart(2, '0')}`;

        let currObj = {
            id: row.DISPLAY_ID,
            propName: 'DIR',
            relProdType: relProdType,
            DT: cr_dt,
            c_name: row.COMPANYNAME,
            c_number: row.CONTACT_NUMBER,
            c_url: row.COMPANY_URL,
            c_w: row.CUSTTYPE_WEIGHT,
            city: row.CITY_NAME,
            fl_name: row.GLCAT_MCAT_FLNAME,
            img_250: row.IMAGE_250X250,
            img_125: row.IMAGE_125X125,
            mcatid: row.MCAT_ID,
            name: row.ITEM_NAME,
            price: row.PRICE, 
            price_f: row.PRICE_F,
            prod_url: row.PDP_URL,
            s_id: row.GLUSR_ID,
            state: row.STATE_NAME,
            unit: row.PRD_SEARCH_MOQ_UNIT_TYPE,
            v_url: row.PROD_VIDEO_PATH,
            tscode: row.tscode,
            locality: row.SDA_GLUSR_USR_LOCALITY,
            // catid: row.state.brd_cat_id,
            mcat_name: row.brd_mct,
            // glusr_custname: row.state.GLUSR_USR_CUST_NAME,
            // prodisq: state.enq_isq_arr,
            img_500x500: row.img_500x500,
            tscode: row.ETO_OFR_COMPANY_TSCODE,
        }; 
        
        let cleanedObj = replaceNullWithEmptyStr(currObj);                 
        return cleanedObj;
    }

    function saveRelData(prodArr) {
        if(prodArr.length==0) {
            return;
        }
        // prodArr- 5 products array

        let relProdArr = [];
        prodArr.forEach(row => {
            let cleanedObj = getObject(row, 'fr');                 
            relProdArr.push(cleanedObj);
        });

        if(relProdArr.length != 0) {
            typeof(sugg.recent)!='undefined' && typeof(sugg.recent)=='function' && sugg.recent({rel_prod: relProdArr});
        }
    }

    function checkSuggAndSaveData() {
        if (sugg && typeof sugg.recent === "function" && IMStore.localStorageLoaded) {
            saveRelData(capsProdsForLS);
        } else {
            setTimeout(checkSuggAndSaveData, 1000);
        }
    }
    setTimeout(checkSuggAndSaveData, 2000);
</script>
