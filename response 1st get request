            function yourcity_search_redirect(){
                var srch_trm = '';
                var list_vw = (window.location.pathname.indexOf('search.mp') > 0 && window.location.href.indexOf('&list_vw=1') >= 0) ? "&list_vw=1" : "";
                var res2='Hyderabad';
                var ipLoc = readCookie('iploc');
                res2 = getparamVal(ipLoc, "gctnm") || "";
                res2= res2.toLowerCase();
                if(res2=='new delhi') res2='delhi';
                if(typeof(dirType) !== 'undefined' && dirType != 's' && typeof(ims.ss) !== 'undefined' && ims.ss !== ''){
                    srch_trm = ims.ss;
                    srch_trm = srch_trm.replace(/&/g,'%26');
                    srch_trm = srch_trm.replace(/ /g,'+');
                }
                if(ims.search_result_language == 'hi'){
                    if(res2=="all-india"||res2=="All India"){
                        window.location = '//' + location.hostname + "/search.mp?ss=" + srch_trm+'&lang=hi';
                        }else{
                            window.location = '//' + location.hostname + "/search.mp?ss=" + srch_trm+'&cq='+res2+'&lang=hi';
                        }
                    }
                else{
                    if(res2=="all-india"||res2=="All India"){
                        window.location = '//' + location.hostname + "/search.mp?ss=" + srch_trm+list_vw;
                        }else{
                            window.location = '//' + location.hostname + "/search.mp?ss=" + srch_trm+'&cq='+res2+list_vw;
                        }
                    }
                }
        </script>
