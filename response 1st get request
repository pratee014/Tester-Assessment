    function loadScriptG() {
        ims.loadScriptGCalled++;
        if(ims.loadScriptGCalled > 3){
            return;
        }
        var slash = "/";
        var u = "utils.imimg.com"+slash;
        var a = "apps.imimg.com"+slash;
        if(location.hostname.match(/dev/)){
            u = 'dev-utils.imimg.com'+slash;
            a = "dev-apps.imimg.com"+slash;
        }else if(location.hostname.match(/stg/)){
            u = 'stg-utils.imimg.com'+slash;
            a = "stg-apps.imimg.com"+slash;
        }
        var p='imsrchui'+slash+'js'+slash;
        var hp='globalhf'+slash+'header'+slash;

