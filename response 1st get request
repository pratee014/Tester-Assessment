        var links = [jquery, hdr, jq_ac, pre, blEnq];
        var catids= [200,173,92,822,123,130,691,20,801,74,758,842,88,65,777,498,370,39];
         
        var catid = 0;
                var flag1=(catids.indexOf(catid) > -1)? 1: 0;
        var mcatstatus = 0;
        var catGridLabel = 0;
        var catGridLabelMainProducts = 0;
        var pageViewStatus = 'list';
        var priceGraph = u+p+'prcgrph.min-v50.js';
        var adWordFlag = 0;
        var isqChkFlg = 0;
        if(ims.loadScriptGCalled===1){
            loadGtm();
            links = [jquery];
        }
        else if(ims.loadScriptGCalled===2 && window.location.href.indexOf('shopnow=1') > 0){
            links = [ hdr, ecomjs, jq_ac,blEnq];
        }else if(ims.loadScriptGCalled===2 && (window.location.pathname.match(/search[1-2]*.mp/i) == null)){
            if (pageViewStatus == 'list'){
                if( mcatstatus != 1){
                    if(ims.graph === '1' && !adWordFlag){
                        links = [hdr, pre, jq_ac, priceGraph];
