 function addloader4(cityform,url,page) {
      if(typeof(cityRedirectToImpcat) !== 'undefined')
      {
        document.getElementById('ldLocat4').style.display='none';
        document.getElementById('locate').style.display='block';
        return cityRedirectToImpcat(cityform,url,page);
