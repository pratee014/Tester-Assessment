    event.initEvent('jqready', true, true);
    function triggerJqueryDefined(fnstring, attemptCounter, time, fnparams,callback) {
        if (typeof(fnparams) === 'undefined') {
            fnparams = [];
        }
        var fn = window[fnstring];
        if (typeof fn === "function") {
            callback();
        } else if (attemptCounter <= ims.maxAttempt) {
            setTimeout(function() {
                triggerJqueryDefined(fnstring, ++attemptCounter, time * 2, fnparams,callback)
            }, time);
        }
    };
