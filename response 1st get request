    function getProdKeys(row) {
        let currObj = {
            Price: row.PRICE_F,
            ProdImage: row.IMAGE_500X500 || row.IMAGE_ORIGINAL || row.IMAGE_250X250 || row.IMAGE_125X125,
            ProdName: row.PC_ITEM_DISPLAY_NAME || row.ITEM_NAME,
            ProdUrl: row.PDP_URL
        }; 
        
        let cleanedObj = replaceNullWithEmptyStr(currObj);                 
        return cleanedObj;
    }

